@startuml
!include style/er.plantuml


!define ENTITY entity

!define PRIMARY_KEY <<PK>>
!define FOREIGN_KEY <<FK>>

ENTITY Codemonkeys {
  + username : STRING <<PRIMARY_KEY>>
  + email : STRING 
  + tag: STRING
}

ENTITY Clienti {
  + username : STRING <<PRIMARY_KEY>>
  + email : STRING 
}

ENTITY Admins {
  + username : STRING <<PRIMARY_KEY>>
}

ENTITY Collaborazioni {
  + id : INT <<PRIMARY_KEY>>
  + codemonkey : STRING <<FOREIGN_KEY>>
}

ENTITY Tags {
  + id : INT <<PRIMARY_KEY>>
}



Codemonkeys "1" --o "N" Collaborazioni : Partecipa a
@enduml
